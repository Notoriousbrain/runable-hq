generator client {
  provider = "prisma-client-js"
  output   = "./src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TitleComponent {
  id        String   @id @default(cuid())
  text      String
  color     String   
  size      Int      
  weight    Int      
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}
